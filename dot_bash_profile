# .bash_profile

# Get the aliases and functions
if [ -f ~/.bashrc ]; then
	. ~/.bashrc
fi

# User specific environment and startup programs
# Autocorrect typos in path names when using `cd`
shopt -s cdspell;

shopt -s autocd;

# Added by Toolbox App
export PATH="$PATH:/home/sanjay/.local/share/JetBrains/Toolbox/scripts"

. "$HOME/.cargo/env"

source /home/sanjay/.config/broot/launcher/bash/br
